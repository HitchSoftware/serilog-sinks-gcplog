{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Gcp" ],
    "MinimumLevel": "Verbose",
    "WriteTo": [
      {
        "Name": "Gcp",
        "Args": {
          "projectID": "PROJECT-ID-12345",
          "resourceType": "gce_instance",
          "logName": "someLogName",
          "labels": {
            "someLabel": "foo"
          },
          "resourceLabels": {
            "someResourceLabel": "bar"
          }
        }
      }
    ]
  }
}
